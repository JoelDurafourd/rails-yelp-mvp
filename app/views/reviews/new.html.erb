<h2>Write a review for <%= @restaurant.name %></h2>
<%= form_with model: [@restaurant, @review] do |f| %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <%= f.submit %>
<% end %>
<%= link_to "Back to Restaurants", "/restaurants", class: "btn btn-primary"%>
